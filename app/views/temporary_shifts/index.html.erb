<div class="button">
  <button type="button" name="name" class="reload btn btn-primary">カレンダーを更新する</button>
  <button type="button" name="name" class="plan btn btn-primary">予定を追加する</button>
  <button type="button" name="name" class="shift btn btn-primary">イエローラインを追加する</button><br>
  <%= link_to "仮削除一覧", deletable_shifts_path , class:"btn btn-primary"%><br>
</div>  
<div id="calendar"></div>

<div id="modal" tabindex="-1" role="dialog">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
      </div>
    </div>
  </div>
</div>

<%= link_to "仮削除一覧", deletable_shifts_path , class:"btn btn-primary"%><br>

<%= link_to "確定する", perfect_temporary_shifts_path, method: :patch, class:"btn btn-primary", data: { confirm: 'Are you sure?' } %>

<%= javascript_pack_tag 'temporary_shifts/calendar' %>
<%= javascript_pack_tag 'shared/index' %>