<div class="row master_plan"> 
      <%= form_with(model: @event, url: create_plan_temporary_shifts_path, remote: true) do |f| %>
         <%= render 'shared/error_messages', object: f.object %>       
         <%= f.label :plan, "予定"%>
         <%= f.text_field :plan, class:'form-control mb-3', required: true %>
        <div class="form-inline text-left">
         <%= f.label :start,"出勤時刻", {class:'control-label'}%>
         <%= f.date_select :start, {default: Date.today}, class: 'form-control bootstrap-date mb-3' %>
        </div>
         <br>
         <%= f.submit "登録",   class: "btn btn-primary js-event-create-btn" %>
      <% end %>
</div>        